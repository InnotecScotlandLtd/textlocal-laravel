{
    "name": "innotecscotlandltd/textlocal-laravel",
    "description": "Package which can allow user to send Text.",
    "homepage": "https://innotec.co.uk",
    "authors": [
        {
            "name": "Shabbir Bharmal",
            "email": "shabbir@innotec.co.uk"
        }
    ],
    "require": {
        "php": "^7.1.8 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "InnotecScotlandLtd\\TextLocal\\": "src/TextLocal/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "InnotecScotlandLtd\\TextLocal\\Providers\\TextLocalServiceProvider"
            ]
        }
    }
}
